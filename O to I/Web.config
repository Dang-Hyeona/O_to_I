<configuration>
 <connectionStrings>
  <add name="sqlexpress2ConnectionString" connectionString="Data Source=(local)\sqlexpress;Initial Catalog=sqlexpress2;Integrated Security=True;Pooling=False"
   providerName="System.Data.SqlClient" />
 </connectionStrings>
 <system.web>
		<compilation debug="true" targetFramework="4.0"/>
		<httpRuntime targetFramework="4.5"/>
   <authentication mode="Forms" >
     <forms name="login" defaultUrl="~/첫화면/default2.aspx" loginUrl="~/첫화면/default.aspx" protection="All" timeout="30"/>
   </authentication>
	</system.web>
<appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
</configuration>